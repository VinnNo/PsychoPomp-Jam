            <string>///System
onGround = OnGround();
if ( state == idle )
{
    if ( nState == true )
    {
        image_speed = 0;
        contactDamage = 0;
        coolDownTimer = 0;
        kLeft = false;
        kRight = false;
        nState = false;
    }
    
    coolDownTimer += 1
    if ( coolDownTimer == delay1 )
    {
        xscale = 1.2;
        yscale = 1.3;
    }
    else if ( coolDownTimer == delay2 )
    {
        xscale = 1.2;
        yscale = 1.3;
    }
    else if ( coolDownTimer == coolDownTimerMax )
    {
        state = trans1;
        nState = true;
    }   
}
else if ( state == walking )
{
    if ( nState == true )
    {
        image_speed = 0.25;
        contactDamage = 1;
        nState = false;
    }
    if ( dir == RIGHT )
    {
        kRight = true;
        kLeft = false;
    }
    else if ( dir == LEFT )
    {
        kRight = false;
        kLeft = true;
    }
    if ( vx == 0 )
    {
        dir = -dir;
        xscale = 0.6;
        yscale = 1.2;
    }
    image_speed = 0.20;
}
else if ( state == trans1 )
{
    if ( nState == true )
    {
        sprite_index = sTurtleStand;
        image_index = 0;
        nState = false;
    }
    image_speed = 0.15;
} 
