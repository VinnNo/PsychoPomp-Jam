            <string>///System
onGround = OnGround();
if ( state == idle )
{
    state = walking;
    nState = true;
}
else if ( state == walking )
{
    if ( nState == true )
    {
        sprite_index = sGunBeyawdaWalk;
        if ( kRightPrev != 0 )
        {
            kRight = true;
            kRightPrev = 0;
            vx = 0.1;
        }
        if ( kLeftPrev != 0 )
        {
            kLeft = true;
            kLeftPrev = 0;
            vx = -0.1;
        }
        nState = false;     
    }
    sightRadius();
    if ( foundHim &amp;&amp; canShoot )
    {
        state = attacking1;
        nState = true;
    }
    else
    {
        enemyWalkerDirection();
        image_speed = 0.15;
    }
}
else if ( state == attacking1 )
{
    if ( nState == true )
    {
        canShoot = false;
        sprite_index = sGunBeyawdaShoot;
        image_index = 0;
        if ( kLeft )
        {
            kLeft = false;
            kLeftPrev = 1;
        }
        if ( kRight )
        {
            kRight = false;
            kRightPrev = 1;
        }
        enemyFacePlayer(0);
        nState = false;
    }
    image_speed = 0.10;
}
else if ( state == isHit )
{
    if ( nState == true )
    {
        if (kLeft)
        kLeft = false;
        if (kRight)
        kRight = false;
        noHit = true;
        sprite_index = sGunBeyawdaHit;
        image_speed = 0;
        alarm[1] = 8;
        nState = false;
    }
} 
