if (frozen == false)
{
    sightRadius()
    if ( foundHim )
    {
        if ( (oPlayer.x &gt; x
        &amp;&amp; facing == RIGHT)
        || (oPlayer.x &lt; x
        &amp;&amp; facing == LEFT) )
        {
            if ( !isAttacking )
            {
                state = attacking1;
                isAttacking = true;
                nState = true;
            }
        }
    }
    
    if ( state == idle )
    {
        if (nState == true)
        {
            sprite_index = sSlaughIdle;
            image_index = 0;
            coolDownTimer = 0;
            vx = 0;
            nState = false;
        }
        
        coolDownTimer += 1;
        if ( coolDownTimer == coolDownTimerMax )
        {
            state = walking;
            nState = true;
        }
        image_speed = 0.1;
    }
    else if ( state == walking )
    {
        if ( nState == true )
        {
            sprite_index = sSlaughWalk;
            image_index = 0;
            nState = false;
        }
        enemyWalkerDirection();
        image_speed = 0.15;
    }
    else if ( state == attacking1 )
    {
        if ( nState == true )
        {
            if ( instance_exists(oPlayer) )
            {
                if ( oPlayer.x &gt; x )
                {
                    image_xscale = 1;
                }
                else
                {
                    image_xscale = -1;
                }
            }
            sprite_index = sSlaughAttack1;
            image_index = 0;
            if ( kRight )
            {
                facingPrev = RIGHT;
                kRight = false;
            }
            else if ( kLeft )
            {
                facingPrev = LEFT;
                kLeft = false;
            }
            kRight = false;
            kLeft = false;
            //enemyFacePlayer(0);
            vx = 0;
            nState = false;
        }
        image_speed = 0.33;
    }
    else if ( state == attacking2 )
    {
        if ( nState == true )
        {
            sprite_index = sSlaughAttack2;
            image_index = 0;
            vx = 0;
            nState = false;
        }
        image_speed = 0.33;
    }
}
else
{
    image_speed = 0;
    vx = 0;
} 
