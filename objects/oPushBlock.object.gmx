            <string>///Persist Stuff
if ( roomPersistID != 0)
{
    if (x != xstart &amp;&amp; y != ystart)
    {
        if (place_meeting(x, y+1, oParSolid) || (
        place_meeting(x, y + 1, oParJumpThru)
        &amp;&amp; !place_meeting(x, y, oParJumpThru))) {
        if (roomPersistID == 1)
        {
            oGame.roomPersistID1 = id;
            if ( oGame.ID1Xstart != x &amp;&amp; oGame.ID1Ystart != y)
            {
                oGame.ID1Xstart = x;
                oGame.ID1Ystart = y;
            }
        }
        else if (roomPersistID == 2)
        {
            oGame.roomPersistID2 = id;
            if ( oGame.ID2Xstart != x &amp;&amp; oGame.ID2Ystart != y)
            {
                oGame.ID2Xstart = x;
                oGame.ID2Ystart = y;
            }
        }
        else if (roomPersistID == 3)
        {
            oGame.roomPersistID3 = id;
            if ( oGame.ID3Xstart != x &amp;&amp; oGame.ID3Ystart != y)
            {
                oGame.ID3Xstart = x;
                oGame.ID3Ystart = y;
            }
        }
        else if (roomPersistID == 4)
        {
            oGame.roomPersistID4 = id;
            if ( oGame.ID4Xstart != x &amp;&amp; oGame.ID4Ystart != y)
            {
                oGame.ID4Xstart = x;
                oGame.ID4Ystart = y;
            }
        }
        else if (roomPersistID == 6)
        {
            oGame.roomPersistID6 = id;
            if ( oGame.ID6Xstart != x &amp;&amp; oGame.ID6Ystart != y)
            {
                oGame.ID6Xstart = x;
                oGame.ID6Ystart = y;
            }
        }
        else if (roomPersistID == 7)
        {
            oGame.roomPersistID7 = id;
            if ( oGame.ID7Xstart != x &amp;&amp; oGame.ID7Ystart != y)
            {
                if ( y &lt; ystart &amp;&amp; !place_meeting(x, y + 1, oPathTimedMover) )
                {
                    oGame.ID7Xstart = x;
                    oGame.ID7Ystart = y;
                }
            }
        }
    }
}
}   
